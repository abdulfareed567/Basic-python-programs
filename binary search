def binary_search(arr, target):
    lb = 0
    ub = len(arr) - 1
    while lb <= ub:
        mid = lb +((ub-lb) // 2 )
        if (arr[mid] == target):
            print(f"element found at index {mid}")
            return mid   
        elif (arr[mid] < target):
            lb = mid + 1
        else:
            ub = mid - 1
    print("element not found")
    return -1  
arr = [1, 3, 5, 7, 9, 11]
target = int(input("enter the element to search:"))
binary_search(arr, target)
